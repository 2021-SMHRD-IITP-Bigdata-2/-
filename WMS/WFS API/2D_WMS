<div id="vmap" style="width:100%;height:370px;left:0px;top:0px"></div>
 
<script type="text/javascript">
	vw.ol3.MapOptions = {
	    basemapType: vw.ol3.BasemapType.GRAPHIC
	  , controlDensity: vw.ol3.DensityType.EMPTY
	  , interactionDensity: vw.ol3.DensityType.BASIC
	  , controlsAutoArrange: true
	  , homePosition: vw.ol3.CameraPosition
	  , initPosition: vw.ol3.CameraPosition
	}; 
	vmap = new vw.ol3.Map("vmap",  vw.ol3.MapOptions); 
	

	let ia = vmap.addNamedLayer("비행정보구역", "lt_c_aisfirc")
	vmap.addLayer(ia)
	let ib = vmap.addNamedLayer("비행제한구역", "lt_c_aisresc")
	vmap.addLayer(ib)
	let ic = vmap.addNamedLayer("비행금지구역", "lt_c_aisprhc")
	vmap.addLayer(ic)
	let id = vmap.addNamedLayer("접근관제구역", "lt_c_aistmac")
	vmap.addLayer(id)
	let id = vmap.addNamedLayer("초경량비행장치공역", "lt_c_aisuac")
	vmap.addLayer(ie)

</script>



