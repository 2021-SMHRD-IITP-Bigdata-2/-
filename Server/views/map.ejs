                <label for="map-content-right-btn">
                    <button class="map-content-right-btn">
                        <i class="fas fa-times fa-fw" aria-hidden="true"></i>
                    </button>
                </label>
                <div class="sidebar">
                    <!-- 결과 -->
                    <div class="predict-result" id="label-container"></div>    
                </div>
            </div>
        </div>
